<?xml version="1.0"?>
<block>
  <name>BLE_pack_collect</name>
  <key>BLE_tools_BLE_pack_collect</key>
  <category>BLE_tools</category>
  <import>import BLE_tools</import>
  <make>BLE_tools.BLE_pack_collect($channel, $accaddr)</make>
  <callback>reset_channel($channel)</callback>
  <callback>reset_accaddr($accaddr)</callback>
 <param>
    <name>channel</name>
    <key>channel</key>
    <type>int</type>
  </param>
 <param>
    <name>accaddr</name>
    <key>accaddr</key>
    <type>string</type>
  </param>


  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
