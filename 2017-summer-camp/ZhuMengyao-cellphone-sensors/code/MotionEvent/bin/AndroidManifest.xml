<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.motionevent"
    android:versionCode="1"
    android:versionName="1.0" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-sdk android:minSdkVersion="7"></uses-sdk>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.drt_3axis_acc.MainActivity1"
            android:label="@string/title_activity_motion_event" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MotionEventActivity"
            android:label="@string/title_activity_motion_event" >
            <intent-filter>
                <action android:name="android.intent.action.MotionEventActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.drt_3axis_acc.MainActivity2"
            android:label="@string/title_activity_motion_event" >
            <intent-filter>
                <action android:name="android.intent.action.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>